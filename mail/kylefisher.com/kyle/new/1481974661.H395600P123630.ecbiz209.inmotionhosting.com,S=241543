  }
  table[class=3D"body"] .panel img {
    display: block !important;
  }
  table[class=3D"body"] .hide-for-small {
    display: none !important;
  }
  table[class=3D"body"] td.hide-for-small {
    display: none !important;
  }
  table[class=3D"body"] td.four-up {
    display: block !important;
    width: 100% !important;
    Margin: 10px 20px 0 0 !important;
  }
  table[class=3D"body"] .social {
    display: block !important;
  }
  table[class=3D"body"] .show-for-small,
  table[class=3D"body"] .hide-for-desktop {
    display: inherit !important;
  }
  table[class=3D"body"] p {
    font-size: 1em !important;
  }
  table[class=3D"body"] .intro p {
    font-weight: bold !important;
    font-size: 1.2em !important;
    -webkit-hyphens: manual !important;
    -moz-hyphens: manual !important;
    hyphens: manual !important;
  }
  table[class=3D"body"] h1, h2, h3, h4, h5 {
    font-size: 1.2em !important;
  }
  table[class=3D"body"] .pre-header p {
    font-size: 0.8em !important;
  }
  table[class=3D"body"] .caption p {
    font-size: 0.8em !important;
    padding-left: 10px;
  }
  table[class=3D"body"] .colophon p, .social p, .q p {
    font-size: 0.7em !important;
    Margin: 10px;
  }
  table[class=3D"body"] .colophon h4, .social h3, .q h2 {
    Margin: 10px !important;
  }
  table[class=3D"body"] p.legal {
    font-size: 0.7em !important;
    Margin: 10px !important;
  }
  table[class=3D"body"] hr {
    display: none !important;
  }
  table[class=3D"body"] .quote {
    width: 100% !important;
    Margin-left: 0px !important;
  }
}
	</style>
</head>
